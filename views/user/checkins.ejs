            <table id="example" class="display" style="width:100%">
                <thead>
                    <tr>
                        <th>Name of Property</th>
                        <th>Location</th>
                        <th>City</th>
                        <th>Amount</th>
                        <th>Checkin Date</th>
                        <th>status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Edffrerrsffdt</td>
                        <td>$7,500</td>
